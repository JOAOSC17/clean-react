{
    "compilerOptions": {
      "target": "es6",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react",
      "rootDir": "src",
      "baseUrl": "src",
      "paths": {
        "@/*": ["*"]
      },
      "strict": true,
      "resolveJsonModule":true,
      "allowJs": true,
    },
    "include": ["src"],
}